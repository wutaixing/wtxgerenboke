<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wtx.myblog.mapper.FeedbackMappwer">

    <select id="getAllFeedback" resultType="com.wtx.myblog.model.FeedBack">
        select * from feedback order by feedbackDate desc
    </select>
    <select id="getAllPrivateWord" resultType="com.wtx.myblog.model.PrivateWord">
        select * from privateword order by publisherDate desc
    </select>
</mapper>

